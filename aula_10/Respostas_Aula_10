<script>
    var qtdNegativos = 0
    for (let i = 0; i < 5; i ++) {
        
        let a = +prompt("Insira um valor")    
        if (a < 0) {
            qtdNegativos++
        }
    }
    
    console.log(qtdNegativos)
</script>